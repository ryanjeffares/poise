func foo(a, b, c) {
    println(a);
    println(b);
    println(c);
}

func main() {
    var range = 0..3;
    foo(...range);

    var a, b, c = 0, 0, 0;
    println(a);
    println(b);
    println(c);

    final ryan, syl, hays = "Ryan", ...["Syl", "Hays"];
    println(ryan);
    println(syl);
    println(hays);
}
