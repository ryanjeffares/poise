import std::float;

func is_none(this None n) {
    return n == none;
}

func main() {
    final make_type = || (final value) => typeof(value);

    println(make_type(typeof(None())())().is_none());

    final f = 10.0;
    println(f);
    println(f.pow(2));
    println(f.sqrt());
    println(f.abs());
}
