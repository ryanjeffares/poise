import std::range;

func main() {
    final min = -10;
    final max = 10;
    for i in min or 5 .. max * 100 by 4 {
        println(i);
    }

    final test_range = || (final range) {
        println(range);
        for value in range {
            println(value);
        }
        println("====");
    };

    var range = 0..=10;
    test_range(range);

    range = -10..20 by 2;
    test_range(range);

    try {
        range = Range(0, 5, 0.2);
        test_range(range);
    } catch e {
        println(e);
    }

    try {
        range = Range(false, "Hello", []);
        test_range(range);
    } catch e {
        println(e);
    }

    println(range.is_infinite_loop());
    println(range.start());
    println(range.end());
    println(range.increment());
    println(range.contains(4));
    println(range.to_list());
}