func throw_exception() => throw Exception("Oops");

func main() {
    try {
        println(foo());
        try {
        } catch error1 {
            try {
                println(foo());
            } catch error2 {
                println(foo());
            }
        }
    } catch error {
        println(error);
    }

    var a = try 2 * 10;
    println(a);
    a = try throw_exception();
    println(a);
}

func foo() => bar();
func bar() => baz();
func baz() => faz();
func faz() => boo();
func boo() => "Hello" / 2;