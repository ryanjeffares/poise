func foo(a, b) {
    return a + b;
}

func make_foo() {
    return foo;
}

func bar(final s, multiplier) {
    return s * multiplier;
}

func main() {
    final foo = make_foo();
    final res = foo(4, 3);
    println(res);

    println(make_foo()(3, 2));

    make_foo();

    println(bar("Hello", 10));
}